{
    "name": "@fui/sheet",
    "version": "1.8.0",
    "main": "bin/wrappers/finesheet.js",
    "module": "bin/wrappers/finesheet.js",
    "types": "bin/wrappers/finesheet.d.ts",
    "browser": "dist/sheet.es5.umd.js",
    "author": "fay",
    "license": "MIT",
    "devDependencies": {
        "@babel/preset-env": "7.4.4",
        "@fui/eslint-plugin": "1.0.20",
        "@fui/npm-publish-hooks": "1.0.1",
        "@types/jest": "24.0.12",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "autoprefixer": "9.6.1",
        "babel-jest": "24.8.0",
        "copy-dir": "1.3.0",
        "cross-env": "5.2.0",
        "eslint": "6.8.0",
        "husky": "1.3.1",
        "jest": "24.8.0",
        "less": "3.9.0",
        "lint-staged": "8.1.5",
        "npm-run-all": "4.1.5",
        "prettier": "1.19.1",
        "rollup": "1.2.2",
        "rollup-plugin-commonjs": "9.3.4",
        "rollup-plugin-json": "4.0.0",
        "rollup-plugin-livereload": "1.0.0",
        "rollup-plugin-node-resolve": "4.2.3",
        "rollup-plugin-postcss": "2.0.3",
        "rollup-plugin-serve": "1.0.1",
        "rollup-plugin-terser": "4.0.4",
        "rollup-plugin-typescript": "1.0.0",
        "rollup-plugin-visualizer": "1.1.0",
        "serve": "10.1.2",
        "ts-jest": "24.0.2"
    },
    "scripts": {
        "build": "tsc && npm run copy && cross-env NODE_ENV=production rollup -c",
        "watch": "rollup -c -w",
        "dev": "cross-env NODE_ENV=development npm-run-all --parallel watch",
        "start": "npm run dev",
        "test": "npm run lint && jest",
        "copy": "copyfiles -u 1 src/**/*.less bin/",
        "lint": "eslint 'src/**/*.ts'",
        "format": "prettier --write 'src/**/*.ts'",
        "replace": "node ./lib/replace/replace.js",
        "buplace": "npm run build && npm run replace",
        "prepublishOnly": "npm run build && node ./lib/prepublish/prepublish.js",
        "postpublish": "node ./lib/postpublish/postpublish.js"
    },
    "files": [
        "dist",
        "bin",
        "types"
    ],
    "dependencies": {
        "@types/lodash-es": "4.17.6",
        "@types/semver": "7.3.10",
        "copyfiles": "2.1.0",
        "hotkeys-js": "3.6.12",
        "is-border-style": "0.1.0",
        "is-color": "1.0.2",
        "lodash-es": "4.17.11",
        "typescript": "3.9.2",
        "uuid": "3.3.2"
    },
    "resolutions": {
        "@types/lodash-es/**/@types/lodash": "4.14.182"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "@pre-push": "npm-run-all --parallel format test"
        }
    },
    "publishConfig": {
        "registry": "https://npm.fineres.com/",
        "tag": "latest"
    },
    "lint-staged": {
        "*.{ts,less}": [
            "eslint --no-error-on-unmatched-pattern"
        ]
    }
}